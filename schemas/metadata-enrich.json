{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$ref": "#/definitions/EnrichmentMetrics",
  "definitions": {
    "EnrichmentMetrics": {
      "type": "object",
      "properties": {
        "success": {
          "type": "object",
          "properties": {
            "count": {
              "type": "number"
            },
            "components": {
              "type": "array",
              "items": {
                "$ref": "#/definitions/ComponentEnrichmentStatus"
              }
            }
          },
          "required": [
            "count",
            "components"
          ],
          "additionalProperties": false
        },
        "fail": {
          "type": "object",
          "properties": {
            "count": {
              "type": "number"
            },
            "components": {
              "type": "array",
              "items": {
                "$ref": "#/definitions/ComponentEnrichmentStatus"
              }
            }
          },
          "required": [
            "count",
            "components"
          ],
          "additionalProperties": false
        },
        "skipped": {
          "type": "object",
          "properties": {
            "count": {
              "type": "number"
            },
            "components": {
              "type": "array",
              "items": {
                "$ref": "#/definitions/ComponentEnrichmentStatus"
              }
            }
          },
          "required": [
            "count",
            "components"
          ],
          "additionalProperties": false
        },
        "total": {
          "type": "number"
        }
      },
      "required": [
        "success",
        "fail",
        "skipped",
        "total"
      ],
      "additionalProperties": false,
      "description": "A metrics data object for storing the final enrichment results. This includes components that were successfully enriched, failed, or skipped. This metrics object can be used to generate the final console or json output."
    },
    "ComponentEnrichmentStatus": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "message": {
          "type": "string"
        },
        "typeName": {
          "type": "string"
        },
        "componentName": {
          "type": "string"
        }
      },
      "required": [
        "componentName",
        "message",
        "typeName"
      ]
    }
  }
}